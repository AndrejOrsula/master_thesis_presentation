% List of IGN colours (unofficial - palette taken from colours used in https://ignitionrobotics.org)
\definecolor{ign@primary1}      {RGB}{120,144,156}
\definecolor{ign@primary2}      {RGB}{091,092,096}
\definecolor{ign@primary3}      {RGB}{003,169,244}

% Refer to `beamer`, `handout`, `second` and `trans` modes
\mode<presentation>
{
    % Setting of beamer colours
    \setbeamercolor{structure}  {fg=ign@primary1}
    \setbeamercolor{frametitle} {fg=white, bg=ign@primary1}
    \setbeamercolor{normal text}{fg=ign@primary2}
}
