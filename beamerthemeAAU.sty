\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamerthemeAAU}[2019/12/17]


% Theme options
\DeclareOptionBeamer{progressstyle} {\PassOptionsToPackage{progressstyle=#1}{beamerouterthemeAAU}}
\DeclareOptionBeamer{rotationcw}    {\PassOptionsToPackage{rotationcw}      {beamerouterthemeAAU}}
